  "deploymentHTML": "<!DOCTYPE html>\n<html>\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Simple Calculator</title>\n    <style>\n        /* Calculator Styles */\nbody {\n    font-family: Arial, sans-serif;\n    max-width: 400px;\n    margin: 50px auto;\n    padding: 20px;\n    background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);\n    color: white;\n    border-radius: 10px;\n}\n\n.calculator {\n    background: rgba(255, 255, 255, 0.1);\n    padding: 20px;\n    border-radius: 10px;\n    backdrop-filter: blur(10px);\n    box-shadow: 0 10px 30px rgba(0, 0, 0, 0.2);\n}\n\ninput {\n    width: 100%;\n    padding: 12px;\n    margin: 10px 0;\n    border: 1px solid rgba(255, 255, 255, 0.3);\n    background: rgba(255, 255, 255, 0.1);\n    color: white;\n    border-radius: 5px;\n    font-size: 18px;\n    transition: border 0.3s;\n}\n\ninput:focus {\n    outline: none;\n    border-color: #ff6b6b;\n}\n\nbutton {\n    background: #ff6b6b;\n    color: white;\n    border: none;\n    padding: 12px 20px;\n    margin: 5px;\n    border-radius: 5px;\n    cursor: pointer;\n    font-size: 16px;\n    transition: all 0.3s;\n    width: 23%;\n}\n\nbutton:hover {\n    background: #ff5252;\n    transform: translateY(-2px);\n    box-shadow: 0 5px 15px rgba(0, 0, 0, 0.2);\n}\n\n.buttons {\n    display: flex;\n    flex-wrap: wrap;\n    justify-content: space-between;\n    margin-top: 15px;\n}\n\n.result {\n    background: rgba(255, 255, 255, 0.2);\n    padding: 15px;\n    border-radius: 5px;\n    margin-top: 20px;\n    font-size: 24px;\n    text-align: center;\n    min-height: 40px;\n    border: 1px solid rgba(255, 255, 255, 0.1);\n}\n\nh2 {\n    text-align: center;\n    margin-top: 0;\n    margin-bottom: 20px;\n    color: white;\n    text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.3);\n}\n\n.success-message {\n    background: rgba(76, 217, 100, 0.2);\n    padding: 10px;\n    border-radius: 5px;\n    margin-top: 15px;\n    text-align: center;\n    border: 1px solid rgba(76, 217, 100, 0.5);\n}\n\n/* Responsive */\n@media (max-width: 480px) {\n    body {\n        margin: 20px auto;\n        padding: 15px;\n    }\n    \n    button {\n        width: 48%;\n        margin-bottom: 10px;\n    }\n}\n    </style>\n</head>\n<body>\n    <!DOCTYPE html>\n<html>\n<head>\n    <title>Simple Calculator Test</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            max-width: 400px;\n            margin: 50px auto;\n            padding: 20px;\n            background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);\n            color: white;\n            border-radius: 10px;\n        }\n        \n        .calculator {\n            background: rgba(255, 255, 255, 0.1);\n            padding: 20px;\n            border-radius: 10px;\n            backdrop-filter: blur(10px);\n        }\n        \n        input {\n            width: 100%;\n            padding: 10px;\n            margin: 10px 0;\n            border: 1px solid rgba(255, 255, 255, 0.3);\n            background: rgba(255, 255, 255, 0.1);\n            color: white;\n            border-radius: 5px;\n            font-size: 18px;\n        }\n        \n        button {\n            background: #ff6b6b;\n            color: white;\n            border: none;\n            padding: 12px 20px;\n            margin: 5px;\n            border-radius: 5px;\n            cursor: pointer;\n            font-size: 16px;\n            transition: all 0.3s;\n            width: 23%;\n        }\n        \n        button:hover {\n            background: #ff5252;\n            transform: translateY(-2px);\n        }\n        \n        .buttons {\n            display: flex;\n            flex-wrap: wrap;\n            justify-content: space-between;\n            margin-top: 15px;\n        }\n        \n        .result {\n            background: rgba(255, 255, 255, 0.2);\n            padding: 15px;\n            border-radius: 5px;\n            margin-top: 20px;\n            font-size: 24px;\n            text-align: center;\n            min-height: 40px;\n        }\n        \n        h2 {\n            text-align: center;\n            margin-top: 0;\n        }\n    </style>\n</head>\n<body>\n    <div class=\"calculator\">\n        <h2>üßÆ Test Calculator</h2>\n        \n        <input type=\"number\" id=\"num1\" placeholder=\"Enter first number\">\n        <input type=\"number\" id=\"num2\" placeholder=\"Enter second number\">\n        \n        <div class=\"buttons\">\n            <button onclick=\"addNumbers()\">‚ûï Add</button>\n            <button onclick=\"subtractNumbers()\">‚ûñ Subtract</button>\n            <button onclick=\"multiplyNumbers()\">‚úñ Multiply</button>\n            <button onclick=\"divideNumbers()\">‚ûó Divide</button>\n        </div>\n        \n        <div class=\"result\" id=\"result\">\n            Result will appear here\n        </div>\n        \n        <button onclick=\"clearAll()\" style=\"width:100%; margin-top:15px; background:#3b82f6;\">\n            üóëÔ∏è Clear All\n        </button>\n        \n        <button onclick=\"showMessage()\" style=\"width:100%; margin-top:10px; background:#10b981;\">\n            ‚ÑπÔ∏è Show Info\n        </button>\n    </div>\n\n    <script>\n        // NORMAL JAVASCRIPT FUNCTIONS (NO window. prefix)\n        // These should auto-convert to global\n        \n        function addNumbers() {\n            const num1 = parseFloat(document.getElementById('num1').value);\n            const num2 = parseFloat(document.getElementById('num2').value);\n            const result = num1 + num2;\n            document.getElementById('result').innerHTML = `Result: ${num1} + ${num2} = <strong>${result}</strong>`;\n        }\n        \n        function subtractNumbers() {\n            const num1 = parseFloat(document.getElementById('num1').value);\n            const num2 = parseFloat(document.getElementById('num2').value);\n            const result = num1 - num2;\n            document.getElementById('result').innerHTML = `Result: ${num1} - ${num2} = <strong>${result}</strong>`;\n        }\n        \n        function multiplyNumbers() {\n            const num1 = parseFloat(document.getElementById('num1').value);\n            const num2 = parseFloat(document.getElementById('num2').value);\n            const result = num1 * num2;\n            document.getElementById('result').innerHTML = `Result: ${num1} √ó ${num2} = <strong>${result}</strong>`;\n        }\n        \n        function divideNumbers() {\n            const num1 = parseFloat(document.getElementById('num1').value);\n            const num2 = parseFloat(document.getElementById('num2').value);\n            \n            if (num2 === 0) {\n                document.getElementById('result').innerHTML = `‚ùå Error: Cannot divide by zero!`;\n            } else {\n                const result = num1 / num2;\n                document.getElementById('result').innerHTML = `Result: ${num1} √∑ ${num2} = <strong>${result}</strong>`;\n            }\n        }\n        \n        function clearAll() {\n            document.getElementById('num1').value = '';\n            document.getElementById('num2').value = '';\n            document.getElementById('result').innerHTML = 'Result will appear here';\n            console.log('Calculator cleared!');\n        }\n        \n        function showMessage() {\n            alert('‚úÖ JavaScript auto-wrapping is working!\\n\\nAll functions are automatically converted to global scope.');\n        }\n        \n        // Arrow function (should also work)\n        const initCalculator = () => {\n            console.log('Calculator initialized!');\n            document.getElementById('result').style.color = '#4ade80';\n        };\n        \n        // Call init when page loads\n        document.addEventListener('DOMContentLoaded', initCalculator);\n    </script>\n</body>\n</html>\n    <script>\n        // === AUTO-WRAPPED JAVASCRIPT ===\n        // All functions are automatically made global\n        \n        // 1. Execute the user's original code first\n        try {\n            (function() {\n                // User's original code (preserved for debugging)\n                // ============================================\n// TEST CALCULATOR - NORMAL JAVASCRIPT FUNCTIONS\n// ============================================\n\n// Regular function declarations (should auto-convert to window.*)\nfunction addNumbers() {\n    const num1 = parseFloat(document.getElementById('num1').value);\n    const num2 = parseFloat(document.getElementById('num2').value);\n    const result = num1 + num2;\n    updateResult(`${num1} + ${num2} = <strong>${result}</strong>`);\n}\n\nfunction subtractNumbers() {\n    const num1 = parseFloat(document.getElementById('num1').value);\n    const num2 = parseFloat(document.getElementById('num2').value);\n    const result = num1 - num2;\n    updateResult(`${num1} - ${num2} = <strong>${result}</strong>`);\n}\n\nfunction multiplyNumbers() {\n    const num1 = parseFloat(document.getElementById('num1').value);\n    const num2 = parseFloat(document.getElementById('num2').value);\n    const result = num1 * num2;\n    updateResult(`${num1} √ó ${num2} = <strong>${result}</strong>`);\n}\n\nfunction divideNumbers() {\n    const num1 = parseFloat(document.getElementById('num1').value);\n    const num2 = parseFloat(document.getElementById('num2').value);\n    \n    if (num2 === 0) {\n        updateResult(`‚ùå Error: Cannot divide by zero!`, 'error');\n    } else {\n        const result = num1 / num2;\n        updateResult(`${num1} √∑ ${num2} = <strong>${result}</strong>`);\n    }\n}\n\n// Arrow function assigned to const (should also work)\nconst clearAll = () => {\n    document.getElementById('num1').value = '';\n    document.getElementById('num2').value = '';\n    updateResult('Result will appear here', 'reset');\n    console.log('Calculator cleared!');\n    \n    // Show success message\n    showTempMessage('Calculator cleared!', 'success');\n};\n\n// Function expression\nconst showMessage = function() {\n    alert('‚úÖ JavaScript auto-wrapping is working!\\n\\n' +\n          'All functions (including arrow functions) are automatically converted to global scope.\\n\\n' +\n          'Check console to see auto-wrapped functions.');\n    \n    // Log to console\n    console.log('Available global functions:');\n    console.log('- addNumbers()');\n    console.log('- subtractNumbers()');\n    console.log('- multiplyNumbers()');\n    console.log('- divideNumbers()');\n    console.log('- clearAll()');\n    console.log('- showMessage()');\n    console.log('- initCalculator()');\n};\n\n// Helper function (not called directly from HTML)\nfunction updateResult(message, type = 'normal') {\n    const resultEl = document.getElementById('result');\n    resultEl.innerHTML = message;\n    \n    if (type === 'error') {\n        resultEl.style.color = '#ff6b6b';\n    } else if (type === 'reset') {\n        resultEl.style.color = 'white';\n    } else {\n        resultEl.style.color = '#4ade80';\n    }\n}\n\n// Another helper function\nfunction showTempMessage(message, type) {\n    // Create message element\n    const msgEl = document.createElement('div');\n    msgEl.className = 'success-message';\n    msgEl.innerHTML = `‚úÖ ${message}`;\n    \n    // Add styles based on type\n    if (type === 'success') {\n        msgEl.style.background = 'rgba(76, 217, 100, 0.2)';\n        msgEl.style.border = '1px solid rgba(76, 217, 100, 0.5)';\n    }\n    \n    // Insert after calculator\n    const calculator = document.querySelector('.calculator');\n    calculator.appendChild(msgEl);\n    \n    // Remove after 2 seconds\n    setTimeout(() => {\n        msgEl.remove();\n    }, 2000);\n}\n\n// Init function (called on page load)\nconst initCalculator = () => {\n    console.log('=== CALCULATOR INITIALIZED ===');\n    console.log('Testing auto-wrapping functionality...');\n    console.log('These functions should be available globally:');\n    console.log('- addNumbers, subtractNumbers, multiplyNumbers, divideNumbers');\n    console.log('- clearAll, showMessage, initCalculator');\n    \n    // Set focus to first input\n    document.getElementById('num1').focus();\n    \n    // Add some styling\n    document.getElementById('result').style.transition = 'all 0.3s ease';\n    \n    console.log('=== READY TO TEST ===');\n    console.log('Click buttons to test if onclick handlers work with normal JavaScript functions');\n};\n\n// Initialize when page loads\ndocument.addEventListener('DOMContentLoaded', initCalculator);\n\n// Export for testing (not needed for auto-wrapping)\nconsole.log('All functions defined normally - auto-wrapping should make them global!');\n            })();\n        } catch(error) {\n            console.error('User code error:', error);\n        }\n        \n        // 2. AUTO-WRAPPED VERSION - Makes everything work automatically\n        (function() {\n            // Processed version with auto-wrapped functions\n            if (typeof declarations === 'function' && !window.declarations) window.declarations = declarations;\nif (typeof addNumbers === 'function' && !window.addNumbers) window.addNumbers = addNumbers;\nif (typeof subtractNumbers === 'function' && !window.subtractNumbers) window.subtractNumbers = subtractNumbers;\nif (typeof multiplyNumbers === 'function' && !window.multiplyNumbers) window.multiplyNumbers = multiplyNumbers;\nif (typeof divideNumbers === 'function' && !window.divideNumbers) window.divideNumbers = divideNumbers;\nif (typeof updateResult === 'function' && !window.updateResult) window.updateResult = updateResult;\nif (typeof showTempMessage === 'function' && !window.showTempMessage) window.showTempMessage = showTempMessage;\nif (typeof clearAll === 'function' && !window.clearAll) window.clearAll = clearAll;\nif (typeof initCalculator === 'function' && !window.initCalculator) window.initCalculator = 