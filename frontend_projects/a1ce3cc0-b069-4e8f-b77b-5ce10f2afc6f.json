unction expression\nconst showMessage = function() { window.showMessage = showMessage;\n    alert('‚úÖ JavaScript auto-wrapping is working!\\n\\n' +\n          'All functions (including arrow functions) are automatically converted to global scope.\\n\\n' +\n          'Check console to see auto-wrapped functions.');\n    \n    // Log to console\n    console.log('Available global functions:');\n    console.log('- addNumbers()');\n    console.log('- subtractNumbers()');\n    console.log('- multiplyNumbers()');\n    console.log('- divideNumbers()');\n    console.log('- clearAll()');\n    console.log('- showMessage()');\n    console.log('- initCalculator()');\n};\n\n// Helper function (not called directly from HTML)\nwindow.updateResult = function(message, type = 'normal') {\n    const resultEl = document.getElementById('result');\n    resultEl.innerHTML = message;\n    \n    if (type === 'error') {\n        resultEl.style.color = '#ff6b6b';\n    } else if (type === 'reset') {\n        resultEl.style.color = 'white';\n    } else {\n        resultEl.style.color = '#4ade80';\n    }\n}\n\n// Another helper function\nwindow.showTempMessage = function(message, type) {\n    // Create message element\n    const msgEl = document.createElement('div');\n    msgEl.className = 'success-message';\n    msgEl.innerHTML = `‚úÖ ${message}`;\n    \n    // Add styles based on type\n    if (type === 'success') {\n        msgEl.style.background = 'rgba(76, 217, 100, 0.2)';\n        msgEl.style.border = '1px solid rgba(76, 217, 100, 0.5)';\n    }\n    \n    // Insert after calculator\n    const calculator = document.querySelector('.calculator');\n    calculator.appendChild(msgEl);\n    \n    // Remove after 2 seconds\n    setTimeout(() => {\n        msgEl.remove();\n    }, 2000);\n}\n\n// Init function (called on page load)\nconst initCalculator = function() { window.initCalculator = initCalculator;\n    console.log('=== CALCULATOR INITIALIZED ===');\n    console.log('Testing auto-wrapping functionality...');\n    console.log('These functions should be available globally:');\n    console.log('- addNumbers, subtractNumbers, multiplyNumbers, divideNumbers');\n    console.log('- clearAll, showMessage, initCalculator');\n    \n    // Set focus to first input\n    document.getElementById('num1').focus();\n    \n    // Add some styling\n    document.getElementById('result').style.transition = 'all 0.3s ease';\n    \n    console.log('=== READY TO TEST ===');\n    console.log('Click buttons to test if onclick handlers work with normal JavaScript functions');\n};\n\n// Initialize when page loads\ndocument.addEventListener('DOMContentLoaded', initCalculator);\n\n// Export for testing (not needed for auto-wrapping)\nconsole.log('All functions defined normally - auto-wrapping should make them global!');\n        })();\n        \n        // 3. Universal event handler for onclick events\n        document.addEventListener('DOMContentLoaded', function() {\n            console.log('Deployed project loaded: \"Simple Calculator\"');\n            \n            // Auto-bind all onclick handlers\n            document.addEventListener('click', function(e) {\n                if (e.target.hasAttribute('onclick')) {\n                    const handler = e.target.getAttribute('onclick');\n                    try {\n                        // Try to execute as is\n                        eval(handler);\n                    } catch(error) {\n                        console.warn('onclick handler error:', error);\n                        // Try to find and call the function\n                        const funcName = handler.replace(/(.*)/, '').trim();\n                        if (typeof window[funcName] === 'function') {\n                            window[funcName]();\n                        }\n                    }\n                }\n            });\n            \n            // Auto-call init() or main() if they exist\n            setTimeout(() => {\n                if (typeof window.init === 'function') window.init();\n                if (typeof window.main === 'function') window.main();\n                if (typeof window.onload === 'function') window.onload();\n            }, 100);\n        });\n        \n        // 4. Make sure DOMContentLoaded fires even if already loaded\n        if (document.readyState === 'loading') {\n            document.addEventListener('DOMContentLoaded', function() {\n                console.log('DOMContentLoaded fired');\n            });\n        } else {\n            console.log('DOM already loaded, triggering event');\n            document.dispatchEvent(new Event('DOMContentLoaded'));\n        }\n    </script>\n</body>\n</html>"
